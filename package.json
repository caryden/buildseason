{
  "name": "buildseason",
  "version": "0.1.0",
  "description": "Open-source team management platform for FTC robotics teams",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.tsx",
    "start": "bun run src/index.tsx",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seed.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@libsql/client": "^0.14.0",
    "better-auth": "^1.1.14",
    "drizzle-orm": "^0.38.3",
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.7.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caryden/buildseason"
  },
  "keywords": [
    "ftc",
    "robotics",
    "team-management",
    "inventory",
    "orders"
  ]
}
